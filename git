git add filename
git commit -m "add message"

git status // on branch and changes
git diff   // view different 

git checkout -b branch_name  //create and move 
git branch branch_name       //create
git checkout branch_name     //change on branh

git branch     //list all branch
git branch -d branch_name //delete branch_name

git merge branch_name   //compact branch_name to on branch
git log --graph         

git tag  //list all tags
git tag tag_name //create tag as the last commit
git tag tag_name commit_id // create tag as commit_id
git show tag_name  
git tag -d tag_name  //delete tag

git push origin tag_name 
git push origin : refs/tags/tag_name

git add -A  提交所有变化
git add -u  提交被修改(modified)和被删除(deleted)文件，不包括新文件(new)
git add .  提交新文件(new)和被修改(modified)文件，不包括被删除(deleted)文件


